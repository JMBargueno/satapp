<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".inventariables.InventariableDetalleActivity">

    <ImageView
        android:id="@+id/fotoDet"
        android:layout_width="153dp"
        android:layout_height="153dp"
        android:layout_marginStart="129dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="129dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/nombreInv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="144dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/fotoDet"
        app:layout_constraintTop_toBottomOf="@+id/fotoDet" />

    <TextView
        android:id="@+id/tipoInv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/nombreInvEdit"
        app:layout_constraintTop_toBottomOf="@+id/nombreInvEdit" />

    <TextView
        android:id="@+id/descInv"
        android:layout_width="320dp"
        android:layout_height="100dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/tipoInvEdit"
        app:layout_constraintTop_toBottomOf="@+id/tipoInvEdit" />

    <TextView
        android:id="@+id/ubInv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/descInv"
        app:layout_constraintTop_toBottomOf="@+id/descInv" />
</androidx.constraintlayout.widget.ConstraintLayout>